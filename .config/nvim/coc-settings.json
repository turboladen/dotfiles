{
  "codeLens.enable": false,
  "cSpell.words": [
    "Agrian's",
    "agrian",
    "chekov",
    "libcompliance",
    "ruleset",
    "rulesets",
    "struct",
    "submodule"
  ],
  "cSpell.enabled": false,
  "diagnostic.errorSign": "✘",
  "diagnostic.infoSign": "ℹ︎",
  "diagnostic.hintSign": "ℹ︎",
  "diagnostic.maxWindowHeight": 10,
  "diagnostic.maxWindowWidth": 100,
  "diagnostic.separateRelatedInformationAsDiagnostics": true,
  "git.addedSign.text": "✚",
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.changedSign.text": "✹",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.changeRemovedSign.text": "⇄",
  "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
  "git.removedSign.text": "✖",
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.hlGroup": "GitGutterDelete",
  "hover.floatMaxHeight": 80,
  "hover.target": "preview",
  "markdownlint.config": {
    "MD013": {
      "line_length": 100
    }
  },
  "languageserver": {
    "steep": {
      "command": "bundle",
      "args": ["exec", "steep", "langserver"],
      "filetypes": ["ruby", "rbs"]
    }
  },
  "list.alignColumns": true,
  "npm.binPath": "/usr/local/bin/npm",
  "prettier.printWidth": 100,
  "prettier.proseWrap": "always",
  "rust-analyzer.cargo.allFeatures": true,
  "rust-analyzer.checkOnSave.allFeatures": true,
  "rust-analyzer.checkOnSave.extraArgs": ["--verbose"],
  "rust-analyzer.hoverActions.linksInHover": true,
  "rust-analyzer.inlayHints.enable": false,
  "rust-analyzer.inlayHints.typeHints": false,
  "rust-analyzer.lens.methodReferences": true,
  "signature.hideOnTextChange": true,
  "signature.maxWindowHeight": 10,
  "signature.maxWindowWidth": 120,
  "signature.preferShownAbove": true,
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  "suggest.enablePreselect": true,
  "suggest.enablePreview": true,
  "suggest.maxPreviewWidth": 100,
  "suggest.minTriggerInputLength": 2,
  "suggest.removeDuplicateItems": true,
  "suggest.numberSelect": true
}
